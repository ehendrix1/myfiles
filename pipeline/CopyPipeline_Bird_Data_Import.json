{
	"name": "CopyPipeline_Bird_Data_Import",
	"properties": {
		"activities": [
			{
				"name": "Copy Bird Avail",
				"description": "move JSON data to Azure SQL",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Copy Bird Trip",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"parallelCopies": 4,
					"enableSkipIncompatibleRow": true,
					"redirectIncompatibleRowSettings": {
						"linkedServiceName": {
							"referenceName": "AzureBlobStorage",
							"type": "LinkedServiceReference"
						},
						"path": "error"
					},
					"dataIntegrationUnits": 4,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"pubTimestamp": "pubdatetime",
							"latitude": "latitude",
							"longitude": "longitude",
							"sumdID": "sumdID",
							"sumdType": "sumdType",
							"chargeLevel": "chargeLevel",
							"sumdGroup": "sumdGroup",
							"costPerMin": "costPerMin"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "AzureBlob_Bird_Avail",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Destination_AzureSQL_Avail",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Copy Bird Trip",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Get Metadata1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 1000
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": true,
					"redirectIncompatibleRowSettings": {
						"linkedServiceName": {
							"referenceName": "AzureBlobStorage",
							"type": "LinkedServiceReference"
						},
						"path": "error"
					}
				},
				"inputs": [
					{
						"referenceName": "AzureBlob_Bird_Trip",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Destination_AzureSQL_Avail",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Get Metadata1",
				"type": "GetMetadata",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"dataset": {
						"referenceName": "AzureBlob_Bird_Trip",
						"type": "DatasetReference"
					}
				}
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}